@model List<MaromFit.Models.Client>
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<table class="table table-bordered table-holver" >

    <thead>
        <tr>
            <th>Nome</th>
            <th>Email</th>
            <th>Plano</th>
        </tr>
    </thead>
    <tbody>
       
        
        @foreach (var Client in Model)
        {
        <tr>
            <td>@Client.Name</td>
            <td>@Client.Mail</td>
            <td>@Client.Plan.Name</td>
        </tr>
        }

    </tbody>
</table>





</table>

